generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id         Int       @id @default(autoincrement())
  username   String    @unique @db.VarChar(255)
  email      String    @unique @db.VarChar(255)
  created_at DateTime? @default(now()) @db.Timestamptz(6)
}
